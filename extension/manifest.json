{
  "manifest_version": 2,
  "name": "add-links",
  "description": "Add predefined links to specified text",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "*://*.google.com/*"
      ],
      "js": [
        "./bundle.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}